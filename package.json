{
  "name": "mesh-ui",
  "version": "0.1.3",
  "description": "3D UI components for VR",
  "author": "Michael Blix <mkeblx@gmail.com>",
  "keywords": [
    "UI",
    "three.js",
    "progress bar",
    "3D",
    "VR"
  ],
  "main": "dist/mesh-ui.min.js",
  "scripts": {
    "build": "rollup -c",
    "build-uglify": "rollup -c && uglifyjs dist/mesh-ui.js > dist/mesh-ui.min.js",

    "build-aframe": "rollup js/aframe-mesh-ui.js --output dist/aframe-mesh-ui.js",
    "build-aframe-uglify": "uglifyjs dist/aframe-mesh-ui.js > dist/aframe-mesh-ui.min.js",
    "aframe": "webpack js/aframe-mesh-ui.js dist/aframe-mesh-ui.bundle.js --optimize-minimize",

    "build-all": "",

    "dev": "rollup -c -w",

    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkeblx/mesh-ui.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mkeblx/mesh-ui/issues"
  },
  "homepage": "https://github.com/mkeblx/mesh-ui",
  "devDependencies": {
    "browserify": "^13.0.0",
    "rollup": "^0.36.0",
    "rollup-watch": "^2.5.0",
    "script-loader": "^0.7.0",
    "uglify-js": "^2.7.3",
    "webpack": "^1.13.0"
  },
  "dependencies": {}
}
